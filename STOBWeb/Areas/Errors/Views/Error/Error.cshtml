@using System.Web.Mvc.Html
@inherits System.Web.Mvc.WebViewPage
@{
    ViewBag.Title = "Error";
    Layout = "~/Areas/Home/Views/Shared/_Layout.cshtml";
}

<section>
    <div class="container">
        <div class="row text-center margin-lv7">
            @{Html.RenderPartial("~/Areas/Errors/Views/Shared/ErrorMessage.cshtml");}
            <div class="col-md-4 col-md-offset-4">
                <div class="row">
                    @Html.ActionLink("Return Home", "Index", "Home", new { area = "Home" }, new { @class = "btn btn-secondary btn-block" })
                </div>
            </div>
        </div>
    </div>
</section>

